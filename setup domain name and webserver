using editor create a flask service 
C:\HWang\MixwellSoftware-Server\VideoService.py

from flask import Flask, send_from_directory, request, abort, render_template_string
import os

# ---------------- é…ç½® ----------------
VIDEO_FOLDER = "C:\HWang\MixwellSoftware-Server\Travel"   # è§†é¢‘æ–‡ä»¶å¤¹è·¯å¾„ï¼ˆå¯ç”¨ç»å¯¹è·¯å¾„ï¼‰
PASSWORD = "huaizhong"    # è‡ªå®šä¹‰è®¿é—®å¯†ç 
PORT = 8080               # æœåŠ¡å™¨ç«¯å£
# --------------------------------------

app = Flask(__name__)

# HTML è¾“å…¥å¯†ç é¡µé¢
HTML_PASSWORD = """
<!doctype html>
<title>å®¶åº­è§†é¢‘</title>
<h1>è¯·è¾“å…¥è®¿é—®å¯†ç </h1>
<form method="post">
  <input type="password" name="pw" placeholder="å¯†ç ">
  <input type="submit" value="è¿›å…¥">
</form>
"""

# HTML è§†é¢‘åˆ—è¡¨é¡µé¢
HTML_LIST = """
<!doctype html>
<title>å®¶åº­è§†é¢‘åˆ—è¡¨</title>
<h1>å®¶åº­è§†é¢‘</h1>
<ul>
{% for video in videos %}
<li>
  <a href="/video/{{video}}" target="_blank">{{video}}</a>
</li>
{% endfor %}
</ul>
"""

@app.route("/", methods=["GET", "POST"])
def index():
    if request.method == "POST":
        if request.form.get("pw") == PASSWORD:
            try:
                videos = [f for f in os.listdir(VIDEO_FOLDER) if f.lower().endswith(('.mp4','.mov','.mkv'))]
            except FileNotFoundError:
                return "è§†é¢‘æ–‡ä»¶å¤¹ä¸å­˜åœ¨ï¼Œè¯·æ£€æŸ¥é…ç½®"
            return render_template_string(HTML_LIST, videos=videos)
        else:
            return "å¯†ç é”™è¯¯"
    return HTML_PASSWORD

@app.route("/video/<path:filename>")
def serve_video(filename):
    # åªå…è®¸è§†é¢‘æ–‡ä»¶è®¿é—®
    if not filename.lower().endswith(('.mp4','.mov','.mkv')):
        abort(403)
    return send_from_directory(VIDEO_FOLDER, filename)

if __name__ == "__main__":
    # å±€åŸŸç½‘è®¿é—® host="0.0.0.0"
    print(f"æœåŠ¡å™¨å¯åŠ¨ï¼Œå±€åŸŸç½‘è®¿é—®åœ°å€: http://ä½ çš„ç”µè„‘IP:{PORT}/")
    print("å®¶äººè®¿é—®æ—¶è¾“å…¥å¯†ç å³å¯çœ‹è§†é¢‘")
    app.run(host="0.0.0.0", port=PORT)

